{
	"entryPoints": ["./src/lib"],
	"entryPointStrategy": "expand",
	"out": "docs",
	"plugin": [],
	"exclude": ["**/*.test.ts", "**/*.spec.ts", "**/node_modules/**", "**/.svelte-kit/**"],
	"excludePrivate": true,
	"excludeProtected": false,
	"excludeExternals": true,
	"readme": "none",
	"theme": "default",
	"name": "sideBar Frontend Documentation",
	"includeVersion": true,
	"sort": ["source-order"],
	"navigation": {
		"includeCategories": true,
		"includeGroups": true
	},
	"categorizeByGroup": true,
	"searchInComments": true,
	"validation": {
		"notExported": true,
		"invalidLink": true,
		"notDocumented": false
	}
}
